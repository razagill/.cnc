function git_branch_name() {    
  git branch 2>/dev/null | awk '/^\*/ { print $2 }'
} 
function git_is_dirty() { 
  echo "$(git diff --quiet 2> /dev/null || echo "%{$fg[red]%}*%{$reset_color%}")"
}


